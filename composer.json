{
    "name": "DevRundown/Skeleton",
    "description": "Skeleton",
    "license": "proprietary",
    "authors": [
        {
            "name": "Bill Mitchell",
            "email": "wkmitch@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "phpmd/phpmd": "^2.5",
        "phploc/phploc": "^3.0",
        "sebastian/phpcpd": "^2.0",
        "slevomat/coding-standard": "^2.0",
        "jakub-onderka/php-console-highlighter": "^0.3.2",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "larapack/dd": "^1.1",
        "bmitch/codor": "^1.0",
        "bmitch/churn-php": "^0.2"
    },
    "autoload": {
        "psr-4": {
            "DevRundown\\Skeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DevRundown\\Skeleton\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/parallel-lint src tests",
            "vendor/bin/phpcs --standard=psr2 src -spn",
            "vendor/bin/phpcs --standard=phpcs.xml src -spn",
            "vendor/bin/phpcs --standard=codor.xml src -spn",
            "vendor/bin/phpunit --debug --coverage-clover=coverage.xml",
            "vendor/bin/phpmd src text codesize,unusedcode,naming",
            "vendor/bin/phploc src --progress",
            "vendor/bin/phpcpd src",
            "vendor/bin/phpunit"
        ]
    }
}
